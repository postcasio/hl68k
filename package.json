{
  "name": "hl68k",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@ts-jison/parser-generator": "^0.3.0",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.16",
    "global": "^4.4.0",
    "lodash": "^4.17.21",
    "ts-jison": "^0.0.6",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build:parser": "ts-jison -t typescript -n HL68k -n HL68k -o src/parser/index.ts grammar/68k.jison",
    "build:tsc": "tsc -p tsconfig.json",
    "build:bin": "cp -r bin/* build/bin && chmod +x build/bin/*",
    "build": "yarn run build:parser && yarn run build:tsc && yarn run build:bin",
    "start": "./build/bin/gsc",
    "test": "yarn run start samples/tutorial/main.as samples/tutorial/tutorial.md"
  },
  "bin": {
    "gsc": "./build/bin/gsc"
  }
}
